{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test package",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${file}",
            "args": ["-goblin.timeout", "3600m"]
        },
        {
            "name": "Run CLI",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}",
            "env": { "NISEKOI_ACCESS_TOKEN": ""},
            "args": ["calc", "CocoaPods"],
            "showLog": true
        }
    ]
}